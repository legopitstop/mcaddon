{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "required": ["format_version", "minecraft:recipe_shapeless"],
  "properties": {
    "format_version": { "type": "string" },
    "minecraft:recipe_shapeless": {
      "type": "object",
      "required": ["description"],
      "properties": {
        "description": {
          "type": "object",
          "required": ["identifier"],
          "properties": {
            "identifier": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "group": {
          "type": "string"
        },
        "ingredients": {
          "type": "array",
          "items": {
            "type": "object"
          }
        },
        "result": {
          "type": ["object", "string"]
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "priority": {
          "type": "integer"
        },
        "unlock": {}
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
