            "additionalProperties": false
          },
          "places_block": {"type": "string"},
          "placement_radius_around_floor": {"type": "integer"},
          "placement_probability_per_valid_position": {"type": "number"},
          "exposed_face": {"type": "string"}
        },
        "additionalProperties": false
      }
    },
    "additionalProperties": false
  }
  