{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "format_version": {
      "type": "string"
    },
    "minecraft:item": {
      "type": "object",
      "required": ["description"],
      "properties": {
        "description": {
          "type": "object",
          "required": ["identifier"],
          "properties": {
            "identifier": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "components": {
          "type": "object"
        },
        "events": {
          "type": "object",
          "additionalProperties": {
            "type": "object"
          }
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
